<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="app.controller.ZookeeperController"
>
    <HBox alignment="BASELINE_RIGHT">
        <Label text="Path:" />
        <TextField fx:id="importPath" promptText="/spring-cloud-config/spring-cloud-gateway:dev" />
        <Button fx:id="importBtn">
            <graphic>
                <FontIcon iconLiteral="ti-import" iconSize="24"/>
            </graphic>
        </Button>
    </HBox>
    <TreeTableView fx:id="keyValueTreeTableView" />
</VBox>

